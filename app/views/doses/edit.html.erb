<div class="container">
  <%= simple_form_for [@cocktail, @dose] do |f| %>
    <%= f.association :ingredient %>
    <%= f.input :description %>
    <%= f.button :submit, class: "btn btn-primary" %>
  <% end %>

<p class="btn btn-outline-primary"><%= link_to "See all yummy Cocktails", cocktails_path %></p>
</div>
